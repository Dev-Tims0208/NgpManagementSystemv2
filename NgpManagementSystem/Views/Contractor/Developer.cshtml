<script>$(document).ready(function () {
        ContractorAnimation();

    });


    inputElements.forEach(function (input) {
        // Skip these elements
        if (input.classList.contains('next') || input.classList.contains('previous') ||
            input.id == "forest_trees_checkbox" || input.id == "forest_trees_year1" ||
            input.id == "fruit_trees_checkbox" || input.id == "fruit_trees_year1" ||
            input.id == "bamboo_checkbox" || input.id == "bamboo_year1" ||
            input.id == "mangrove_checkbox" || input.id == "mangrove_year1") {
            return;
        }

        // Ccheckbox
        var nullCheckbox = document.createElement('input');
        nullCheckbox.setAttribute('type', 'checkbox');
        nullCheckbox.setAttribute('id', input.id + '_null_checkbox');
        nullCheckbox.setAttribute('onchange', 'handleNullCheckbox("' + input.id + '", this)');

        // Label
        var label = document.createElement('label');
        label.setAttribute('for', input.id + '_null_checkbox');
        label.textContent = 'N/A';

        // Insert the new checkbox and label after the current input or select element
        input.insertAdjacentElement('afterend', label);
        input.insertAdjacentElement('afterend', nullCheckbox);
    });

    function handleNullCheckbox(inputId, nullCheckbox) {
        var inputField = document.getElementById(inputId);

        if (nullCheckbox.checked) {
            inputField.value = '';
            inputField.disabled = true; // Disable editing
            inputField.style = 'background-color: #ada5a5;';
        } else {
            inputField.disabled = false; // Enable editing
            inputField.style = 'background-color: #ECEFF1;';
        }
    }

    function toggleCommodityTextField(checkbox) {
        var textFieldId = checkbox.id.replace("_checkbox", "_year1");
        var textField = document.getElementById(textFieldId);

        if (checkbox.checked) {
            textField.style.display = "block";
            textField.focus();
        } else {
            textField.style.display = "none";
            textField.value = "";
        }
    }</script>



